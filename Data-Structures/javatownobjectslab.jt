//
// Calculator is used to add, subtract, multiply, square, findOdd number, 
// find max of 2 numbers, and absolute value of a number
// 
// @author Anagha Ram
// @version Sep 2, 2020

public class Calculator
{
   public Calculator()
   {
   }
	
   // Adds 2 numbers
   // @param num1 the first number
   // @param num2 the second number
   // @return the sum
   public add(num1, num2)
   {
      return num1+num2;
   }

   // Subtracts 2 numbers
   // @param num1 the first number
   // @param num2 the second number
   // @return the difference

   public subtract(num1, num2)
   {
      return num1-num2;
   }

   // Multiplies 2 numbers
   // @param num1 the first number
   // @param num2 the second number
   // @return the product


   public multiply(num1, num2)
   {
      return num1*num2;
   }

   // Squares a number
   // @param num the number
   // @return the square of the number


   public square(num)
   {
       return num*num;
   }


   // Finds if the number is odd
   // @param num the number
   // @return true if the number is odd
   //     else false
 
   public isOdd(num)
   {
        return (num%2!=0); 
   }

   // Finds the max of 2 numbers
   // @param num1 the first number
   // @param num2 the second number
   // @return the max of the 2 numbers   


   public max(num1, num2)
   {
        if (num1> num2)
        {
            return num1;        
        }
        if (num2>num1)
        {               
            return num2;
        }
        return num1;
   } 
}

//
// Box is used to save a value and return it when needed
// 
// @author Anagha Ram
// @version Sep 2, 2020


public class Box
{

private value; 

   // Constructs a new Box class 
   // @param val value to be stored

   public Box(val)
   {
	value= val;
   }
   // gets the stored value
   // @return the stored value  

   public getValue()
   {
	return value;
   }

   // Sets the value to be stored 
